<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
            <img class="animation__shake" src="~/img/logo.png" alt="FourN" height="350" width="350">
        </div>

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Navbar Search -->
                <li data-toggle="tooltip" data-placement="bottom" title="Tasks are NEW" class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i style="font-size: 25px; font-weight: bolder;" class="far fa-lightbulb"></i>
                        <span id="notiNew" class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
                <li data-toggle="tooltip" data-placement="bottom" title="Tasks are PROCESSING" class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i style="font-size: 25px; font-weight: bolder;" class="fas fa-spinner"></i>
                        <span id="notiProcess" class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
                <li data-toggle="tooltip" data-placement="bottom" title="Tasks are CHECKING" class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i style="font-size: 25px; font-weight: bolder;" class="fas fa-tasks"></i>
                        <span id="notiChecking" class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
                <li data-toggle="tooltip" data-placement="bottom" title="Tasks are DEBUG" class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i style="font-size: 25px; font-weight: bolder;" class="fas fa-bug"></i>
                        <span id="notiDebug" class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
                <li data-toggle="tooltip" data-placement="bottom" title="Tasks are DONE" class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i style="font-size: 25px; font-weight: bolder;" class="fas fa-check-circle"></i>
                        <span id="notiDone" class="badge badge-danger navbar-badge"></span>
                    </a>
                </li>
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown li-badge">
                    <a class="nav-link a-dropdown" data-toggle="dropdown" href="#">
                        <i class="far fa-bell" style="font-size: 25px;"></i>
                        <span class="badge badge-danger navbar-badge count-badge">0</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right dropdown-badge" style="max-width: 900px;">
                    </div>
                </li>
                @*<li class="nav-item">
                    <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                        <i class="fas fa-expand-arrows-alt"></i>
                    </a>
                </li>
