<script>
    $(document).ready(function () {
        $.ajax({
            url: '/Module4/Calendar/GetAllNotification',
            dataType: 'json',
            //data: {
            //    content: text,
            //    groupid: groupid
            //},
            type: "GET",
            beforeSend: function () {

            },
            complete: function () {

            },
            success: function (json) {
                if (json['status'] == true) {
                    if (json['count'] > 0) {
                        var numCount = Number($('span.count-badge').text());
                        numCount = numCount + json['countNoti'];
                        $('span.count-badge').css('display', 'block');
                        $('span.count-badge').text(numCount)
                        $.each(json['result'], function (key, value) {
                            if (value['notitype'] == 'message') {
                                if (value['notiread'] == 0)
                                    var html = '<a class="dropdown-item" href="/Module4/Message" data-id="' + value['notiid'] + '" style="color:red;font-weight: 700"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                else
                                    var html = '<a class="dropdown-item" href="/Module4/Message" data-id="' + value['notiid'] + '" style="color:black;font-weight: 100"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                $('div.dropdown-badge').append(html);
                            }
                            else if (value['notitype'] == 'meeting') {
                                if (value['notiread'] == 0)
                                    var html = '<a class="dropdown-item" href="/Module4/Calendar" data-id="' + value['notiid'] + '" style="color:red;font-weight: 700"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                else
                                    var html = '<a class="dropdown-item" href="/Module4/Calendar" data-id="' + value['notiid'] + '" style="color:black;font-weight: 100"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                $('div.dropdown-badge').append(html);
                            }
                            else {
                                if (value['notiread'] == 0)
                                    var html = '<a class="dropdown-item" href="/Module4/Calendar" data-id="' + value['notiid'] + '" style="color:red;font-weight: 700"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                else
                                    var html = '<a class="dropdown-item" href="/Module4/Calendar" data-id="' + value['notiid'] + '" style="color:black;font-weight: 100"><i class="fas fa-envelope mr-2"></i> You have a new <span id="notify" >' + value['notitype'] + '</span> from ' + value['usersSend']['username'] + ' at ' + value['noticreate'] + '</a ><div class="dropdown-divider"></div>';
                                $('div.dropdown-badge').append(html);
                            }
                        });
                    }
                    else {
                        var html = '<a class="dropdown-item" data-id="0" href="#">No notification.</a ><div class="dropdown-divider"></div>';
                        $('.dropdown-badge').append(html);
                    }
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
            },
        });//end ajax
    });
</script>
