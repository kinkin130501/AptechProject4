    <script>
        $('a.a-dropdown').click(function () {
            var status = $(".dropdown-badge").css('display');
            if (status == "block") {
                $(".dropdown-badge").css("display", "none");
                $('.dropdown-badge a').css('color', 'black');
                $('.dropdown-badge a').css('font-weight', '100');
                $('.dropdown-badge a i').css('color', 'black');
            }
            else {
                $(".dropdown-badge").css("display", "block");
            }
        });
        $('li.li-badge').click(function () {
            var status = $(".dropdown-badge").css('display');
            if (status == "block") {
                $(".dropdown-badge").css("display", "none");
            }
            var id = $(this).attr("data-id");
            $.ajax({
                url: '/Module4/Calendar/ReadAllNotification',
                dataType: 'json',
                data: {
                    id: id
                },
                type: "POST",
                beforeSend: function () {
                },
                complete: function () {

                },
                success: function (json) {
                    if (json['status'] == true) {
                        $('span.count-badge').text(0)
                        //$('span.count-badge').css('display', 'none');
                        $('.dropdown-badge').css('display', 'block');
                        //$('.dropdown-badge a').css('color', 'black');
                        //$('.dropdown-badge a').css('font-weight', '100');
                        //$('.dropdown-badge a i').css('color', 'black');
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                },
            });//end ajax
        });
        $('li.Logout').click(function () {
            $.ajax({
                url: '/Module4/Calendar/Logout',
                dataType: 'json',
                //data: {
                //    content: text,
                //    groupid: groupid
                //},
                type: "POST",
                beforeSend: function () {
                },
                complete: function () {

                },
                success: function (json) {
                    if (json['status'] == true) {
                        location = "https://localhost:44316/Authentication/Login"
                    }
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
                },
            });//end ajax
        });
    </script>
