@model IEnumerable<GroupViewModel> 
<table class="table table-striped projects">
    <thead>
        <tr>
            <th style="width: 1%">
                #
            </th>
            <th style="width: 20%">
                Project Name
            </th>
            <th style="width: 30%">
                Team Members
            </th>
            <th style="width: 20%">
                Project
            </th>
            <th style="width: 20%">
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        #
    </td>
    <td>
        @item.GroupName
    </td>
    <td>
        <ul class="list-inline">
            @if (item.Users != null && item.Users.Any())
            {
@foreach (var user in item.Users)
{
<li class="list-inline-item">
    <img alt="Avatar" class="table-avatar" src="@user.Avatar">
</li>}}

        </ul>
    </td>
    <td>
        @item.ProjectName
    </td>
    <td class="project-actions text-right">
        <a class="btn btn-primary btn-sm" href="Group/Detail/@item.GroupId">
            <i class="fas fa-folder">
            </i>
            View
        </a>
        <a class="btn btn-danger btn-sm" data-toggle="modal" onclick="DeleteGroup(@item.GroupId)">
            <i class="fas fa-trash">
            </i>
            Delete
        </a>
    </td>
</tr>}

    </tbody>
</table>
<script src="~/js/notify.js"></script>